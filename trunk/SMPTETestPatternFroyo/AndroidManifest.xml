<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="ca.jvsh.smpte" android:versionCode="1" android:versionName="1.0.20100811.1">

	<uses-sdk android:minSdkVersion="8" />
	<uses-feature android:name="android.software.live_wallpaper" />

	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:permission="android.permission.BIND_WALLPAPER" android:thumbnail="@drawable/icon">
		<service android:name=".SMPTE" android:label="@string/app_name" android:icon="@drawable/icon" android:thumbnail="@drawable/icon">
			<intent-filter>
				<action android:name="android.service.wallpaper.WallpaperService" />
			</intent-filter>
			<meta-data android:name="android.service.wallpaper"
				android:resource="@xml/smpte" />
		</service>
		<activity android:label="@string/smpte_settings"
			android:name=".SMPTESettings" android:theme="@android:style/Theme.Light.WallpaperSettings"
			android:exported="true" android:icon="@drawable/icon" android:thumbnail="@drawable/icon">
		</activity>
	</application>
</manifest> 